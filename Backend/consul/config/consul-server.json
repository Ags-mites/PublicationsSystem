{
  "datacenter": "dc1",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "node_name": "consul-server-1",
  "bind_addr": "0.0.0.0",
  "client_addr": "0.0.0.0",
  "retry_join": ["consul-server-1"],
  "server": true,
  "bootstrap_expect": 1,
  "ui_config": {
    "enabled": true
  },
  "connect": {
    "enabled": true
  },
  "enable_script_checks": true,
  "enable_local_script_checks": true,
  "acl": {
    "enabled": false,
    "default_policy": "allow"
  },
  "encrypt": "",
  "verify_incoming": false,
  "verify_outgoing": false,
  "ca_file": "",
  "cert_file": "",
  "key_file": "",
  "ports": {
    "grpc": 8502
  },
  "telemetry": {
    "prometheus_retention_time": "30s",
    "disable_hostname": true
  },
  "services": [
    {
      "name": "consul",
      "tags": ["consul", "cluster"],
      "port": 8500,
      "check": {
        "http": "http://localhost:8500/v1/status/leader",
        "interval": "10s"
      }
    }
  ]
}